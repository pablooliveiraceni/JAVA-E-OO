package Exercicios;

public class programaComBreak {
	
	public static void main(String[] args) {
		
		for(int linha = 0; linha < 5; linha ++) {
			for(int coluna = 0; coluna < 5; coluna++) {
				if(coluna > linha) {
					System.out.println(coluna + linha);
					break;
				}	
			
			}
			System.out.printl("");
			
			
			
		}
		
	}

}
